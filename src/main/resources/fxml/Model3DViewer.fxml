<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.control.Button?>
<?import javafx.geometry.Insets?>

<VBox spacing="10" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/20.0.1" fx:controller="tn.esprit.workshop.gui.Model3DViewerController">
    <padding>
        <Insets bottom="10" left="10" right="10" top="10" />
    </padding>
    
    <AnchorPane fx:id="viewerContainer" VBox.vgrow="ALWAYS" style="-fx-background-color: #2f2f2f;" />
    
    <Button text="Fermer" onAction="#handleClose" style="-fx-background-color: #cd853f; -fx-text-fill: white;" />
</VBox> 